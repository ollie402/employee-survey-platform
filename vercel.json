{
  "version": 2,
  "functions": {
    "api/*.js": {
      "maxDuration": 10
    }
  },
  "rewrites": [
    {
      "source": "/chat/:slug",
      "destination": "/chat.html"
    },
    {
      "source": "/survey/:id",
      "destination": "/survey.html"
    },
    {
      "source": "/setup",
      "destination": "/setup.html"
    }
  ]
}
